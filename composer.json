{
    "name": "fwahlqvist/wdev-presentation",
    "description": "ZF2 Module implementing reveal.js presentation",
    "repositories": {
        "symfony-unofficial": {
            "type": "package",
            "package": {
                "name": "fos/facebookbundle",
                "version": "2.0",
                "source": {
                    "url": "git://github.com/FriendsOfSymfony/FOSFacebookBundle.git",
                    "type": "git",
                    "reference": "origin/2.0"
                }
            }
        }
    }

    "keywords": [
        "zf2",
        "zend",
        "module",
        "reveal.js",
        "Presentation"
    ],
    "homepage": "https://github.com/fwahlqvist/WdevPresentation",
    "authors": [
        {
            "name": "Fredrik Wahlqvist",
            "email": "fwahlqvist@gmail.com",
            "homepage": "http://wahlqvist.co.uk/"
        }
    ],                
    "require": {
        "php": ">=5.3.3",
        "zendframework/zendframework": "2.*",
        "fos/facebookbundle": "2.0.*"
    },
    "autoload": {
        "psr-0": {
            "WdevPresentation": "src/"
        },
        "classmap": [
            "./"
        ]
    }
}
